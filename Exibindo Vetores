
public class app {
    public static void main(String[] args) {
        int[] num = new int[5];
        // declrando um array
        num[0]=10;
        num[1]=20;
        num[2]=30;
        num[3]=40;
        num[4]=50;
        //preechendo um Array.
        for(int i=0;i<6;i++){
            // Exibindo um array
            System.out.printf("%d\n", num[i]);
        }

        }
}
